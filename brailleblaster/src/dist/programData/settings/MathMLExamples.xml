<?xml version="1.0" encoding="UTF-8"?>
<dtbook version="2005-3" xml:lang="en"
	xmlns="http://www.daisy.org/z3986/2005/dtbook/"
	xmlns:m="http://www.w3.org/1998/Math/MathML">
	<head />
	<book>
		<bodymatter>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="preceding subscript superscript">
				<mstyle displaystyle="true">
					<mrow>
						<msubsup>
							<mrow>
								<mtext></mtext>
							</mrow>
							<mn>92</mn>
							<mn>238</mn>
						</msubsup>
					</mrow>
					<mi>U</mi>
				</mstyle>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="mixed fraction">
				<semantics>
					<mstyle displaystyle="true">
						<mfrac>
							<mrow>
								<mn>2</mn>
								<mfrac>
									<mn>1</mn>
									<mn>2</mn>
								</mfrac>
							</mrow>
							<mrow>
								<mn>4</mn>
								<mfrac>
									<mn>3</mn>
									<mn>4</mn>
								</mfrac>
							</mrow>
						</mfrac>
					</mstyle>
					<annotation encoding="text/x-asciimath">frac(2 frac 1 2)(4 frac 3 4)
					</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="bevelled fraction">
				<semantics>
					<mstyle displaystyle="true">
						<mfrac bevelled="true">
							<mn>1</mn>
							<mn>2</mn>
						</mfrac>
					</mstyle>
					<annotation encoding="text/x-asciimath">bfrac 1 2</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="overbar">
				<semantics>
					<mstyle displaystyle="true">
						<mover>
							<mi>x</mi>
							<mo>&#xAF;</mo>
						</mover>
					</mstyle>
					<annotation encoding="text/x-asciimath">bar x</annotation>
				</semantics>
			</math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" example="vector">
                <semantics>
                    <mstyle displaystyle="true">
                        <mover>
                            <mi>x</mi>
                            <mo>&#x2192;</mo>
                        </mover>
                    </mstyle>
                    <annotation encoding="text/x-asciimath">vec x</annotation>
                </semantics>
            </math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="square root">
				<semantics>
					<mstyle displaystyle="true">
						<msqrt>
							<mrow>
								<mn>64</mn>
							</mrow>
						</msqrt>
					</mstyle>
					<annotation encoding="text/x-asciimath">sqrt(64)</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="radical">
				<semantics>
					<mstyle displaystyle="true">
						<mroot>
							<mrow>
								<mn>18</mn>
								<mo>+</mo>
								<mn>9</mn>
							</mrow>
							<mrow>
								<mn>3</mn>
							</mrow>
						</mroot>
					</mstyle>
					<annotation encoding="text/x-asciimath">root(3)(18+9)</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="exponent">
				<semantics>
					<mstyle displaystyle="true">
						<msup>
							<mi>x</mi>
							<mn>2</mn>
						</msup>
					</mstyle>
					<annotation encoding="text/x-asciimath">x^2</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="complex fraction">
				<semantics>
					<mstyle displaystyle="true">
						<mfrac>
							<mrow>
								<mn>123</mn>
							</mrow>
							<mrow>
								<mn>12</mn>
								<mo>+</mo>
								<mn>3</mn>
							</mrow>
						</mfrac>
					</mstyle>
					<annotation encoding="text/x-asciimath">(123)/(12+3)</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="log base 2">
				<semantics>
					<mstyle displaystyle="true">
						<mrow>
							<msub>
								<mi>log</mi>
								<mn>2</mn>
							</msub>
						</mrow>
					</mstyle>
					<annotation encoding="text/x-asciimath">log_2</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="fraction">
				<semantics>
					<mstyle displaystyle="true">
						<mfrac>
							<mn>12</mn>
							<mi>a</mi>
						</mfrac>
					</mstyle>
					<annotation encoding="text/x-asciimath">12/a</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="taylor series">
				<mrow>
					<mi>f</mi>
					<mrow>
						<mo>(</mo>
						<mi>x</mi>
						<mo>)</mo>
					</mrow>
				</mrow>
				<mo>=</mo>
				<mrow>
					<munderover>
						<mo>&#x2211;</mo>
						<mrow>
							<mi>n</mi>
							<mo>=</mo>
							<mn>0</mn>
						</mrow>
						<mo>&#x221E;</mo>
					</munderover>
				</mrow>
				<mrow>
					<mo>(</mo>
					<mfrac>
						<mrow>
							<mrow>
								<msup>
									<mi>f</mi>
									<mi>n</mi>
								</msup>
								<mi>a</mi>
							</mrow>
						</mrow>
						<mrow>
							<mi>n</mi>
							<mo>!</mo>
						</mrow>
					</mfrac>
					<mo>)</mo>
				</mrow>
				<msup>
					<mrow>
						<mo>(</mo>
						<mi>x</mi>
						<mo>-</mo>
						<mi>a</mi>
						<mo>)</mo>
					</mrow>
					<mi>n</mi>
				</msup>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="quadratic">
				<mi>x</mi>
				<mo>=</mo>
				<mfrac>
					<mrow>
						<mo>-</mo>
						<mi>b</mi>
						<mo>&#xB1;</mo>
						<msqrt>
							<mrow>
								<msup>
									<mi>b</mi>
									<mn>2</mn>
								</msup>
								<mo>-</mo>
								<mn>4</mn>
								<mi>a</mi>
								<mi>c</mi>
							</mrow>
						</msqrt>
					</mrow>
					<mrow>
						<mn>2</mn>
						<mi>a</mi>
					</mrow>
				</mfrac>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="integrate natural log">
				<mo>&#x222B;</mo>
				<mfrac>
					<mn>1</mn>
					<mi>x</mi>
				</mfrac>
				<mrow>
					<mi>d</mi>
					<mi>x</mi>
				</mrow>
				<mo>=</mo>
				<mrow>
					<mi>ln</mi>
					<mrow>
						<mo>|</mo>
						<mrow>
							<mi>x</mi>
						</mrow>
						<mo>|</mo>
					</mrow>
				</mrow>
				<mo>+</mo>
				<mi>C</mi>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="pythagorean identity">
				<mrow>
					<msup>
						<mi>sin</mi>
						<mn>2</mn>
					</msup>
					<mrow>
						<mo>(</mo>
						<mi>&#x3B8;</mi>
						<mo>)</mo>
					</mrow>
				</mrow>
				<mo>+</mo>
				<mrow>
					<msup>
						<mi>cos</mi>
						<mn>2</mn>
					</msup>
					<mrow>
						<mo>(</mo>
						<mi>&#x3B8;</mi>
						<mo>)</mo>
					</mrow>
				</mrow>
				<mo>=</mo>
				<mn>1</mn>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="column vectors">
				<mrow>
					<mo>(</mo>
					<mtable>
						<mtr>
							<mtd>
								<mi>a</mi>
							</mtd>
							<mtd>
								<mi>b</mi>
							</mtd>
						</mtr>
						<mtr>
							<mtd>
								<mi>c</mi>
							</mtd>
							<mtd>
								<mi>d</mi>
							</mtd>
						</mtr>
					</mtable>
					<mo>)</mo>
				</mrow>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="script order">
				<mrow>
					<msubsup>
						<mo>&#x222B;</mo>
						<mn>0</mn>
						<mn>1</mn>
					</msubsup>
				</mrow>
				<mrow>
					<mi>f</mi>
					<mrow>
						<mo>(</mo>
						<mi>x</mi>
						<mo>)</mo>
					</mrow>
				</mrow>
				<mrow>
					<mi>d</mi>
					<mi>x</mi>
				</mrow>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="complex subscripts">
				<munder>
					<mo>lim</mo>
					<mrow>
						<mi>x</mi>
						<mo>&#x2192;</mo>
						<mo>&#x221E;</mo>
					</mrow>
				</munder>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="matrices">
				<mrow>
					<mo>[</mo>
					<mtable>
						<mtr>
							<mtd>
								<mi>a</mi>
							</mtd>
							<mtd>
								<mi>b</mi>
							</mtd>
						</mtr>
						<mtr>
							<mtd>
								<mi>c</mi>
							</mtd>
							<mtd>
								<mi>d</mi>
							</mtd>
						</mtr>
					</mtable>
					<mo>]</mo>
				</mrow>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="sans-serif">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="sans-serif">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">sf "AaBbCc"</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="fraktur">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="fraktur">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">fr "AaBbCc"</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="monospace">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="monospace">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">tt "AaBbCc"</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="script">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="script">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">cc "AaBbCc"</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="double-struck">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="double-struck">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">bbb "AaBbCc"</annotation>
				</semantics>
			</math>
			<math xmlns="http://www.w3.org/1998/Math/MathML" example="bold">
				<semantics>
					<mstyle displaystyle="true">
						<mstyle mathvariant="bold">
							<mrow>
								<mtext>AaBbCc</mtext>
							</mrow>
						</mstyle>
					</mstyle>
					<annotation encoding="text/x-asciimath">bb "AaBbCc"</annotation>
				</semantics>
			</math>
		</bodymatter>
	</book>
</dtbook>